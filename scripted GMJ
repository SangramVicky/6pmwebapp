 pipeline{
	agent any
	 	stages{
			stage(kode_cloud){
				steps{
			  git credentialsId: 'github', url: 'https://github.com/SangramVicky/6pmwebapp'
			}
		stage(build){
			steps{
				script{
				def mvnHome =  tool name: 'maven11', type: 'maven'
			sh label: '', script: "${mvnHome}/bin/mvn clean package"
			}
			}
			}	
		}
		}
		
			
 }
